  let width = $(\"body\").width();\n          if (width >= 1200) {\n            let fontsize = width / 1920 * 40;\n            $(\"html\").css(\"font-size\", `${fontsize}px`)\n          }\n        })()\n      }\n    }\n\n  }\n</script>\n"]}]}