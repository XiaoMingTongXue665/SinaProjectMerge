bF,IAAAA,MAAM,EAAE;AACNQ,MAAAA,KAAK,EAALA,KADM,CACA;;AADA,KAHK;AAMbJ,IAAAA,IAAI,EAAJA;AANa,GAAD,CAAd;AAQD,CATM;AAWP;AACA;AACA;;AACA,OAAO,IAAMK,aAAa,GAAG,SAAhBA,aAAgB,CAACH,SAAD,EAAYF,IAAZ,EAAoC;AAAA,MAAlBI,KAAkB,uEAAV,KAAU;AAC/D,SAAOV,OAAO,CAAC;AACbG,IAAAA,MAAM,EAAE,KADK;AAEbC,IAAAA,GAAG,8BAAuBI,SAAvB,CAFU;AAGbN,IAAAA,MAAM,EAAE;AACNQ,MAAAA,KAAK,EAALA,KADM,CACA;;AADA,KAHK;AAMbJ,IAAAA,IAAI,EAAJA;AANa,GAAD,CAAd;AAQD,CATM;AAWP;AACA;AACA;;AACA,OAAO,IAAMM,UAAU,GAAG,SAAbA,UAAa,CAAAJ,SAAS,EAAI;AACrC,SAAOR,OAAO,CAAC;AACbG,IAAAA,MAAM,EAAE,KADK;AAEbC,IAAAA,GAAG,8BAAuBI,SAAvB;AAFU,GAAD,CAAd;AAID,CALM;AAOP;AACA;AACA;;AACA,OAAO,IAAMK,mBAAmB,GAAG,SAAtBA,mBAAsB,CAACL,SAAD,EAAYM,YAAZ,EAA6B;AAC9D,SAAOd,OAAO,CAAC;AACbG,IAAAA,MAAM,EAAE,KADK;AAEbC,IAAAA,GAAG,EAAE,0BAFQ;AAGbF,IAAAA,MAAM,EAAE;AACN;AACAa,MAAAA,UAAU,EAAEP;AAFN,KAHK;AAObF,IAAAA,IAAI,EAAE;AACJU,MAAAA,aAAa,EAAEF;AADX;AAPO,GAAD,CAAd;AAWD,CAZM","sourcesContent":["/**\n * 文章相关请求模块\n */\nimport request from '@/utils/request'\n\n/**\n * 获取文章列表\n */\nexport const getPhysicalListApi = params => {\n  return request({\n    method: 'GET',\n    url: '/assets/personal/list/',\n    // Body 参数使用 data 设置\n    // Query 参数使用 params 设置\n    // Headers 参数使用 headers 设置\n    params\n  })\n}\n\n/**\n * 获取文章频道\n */\nexport const getStatusAPI = (data) => {\n  return request({\n    method: 'POST',\n    url: '/power/instances/status',\n    data\n  })\n}\n\n/**\n * 删除文章\n */\nexport const deleteArticle = articleId => {\n  return request({\n    method: 'DELETE',\n    // 接口文档中的写的路径参数需要在 url 中传递\n    // 凡是看见接口路径中有的 :xxx 格式的字段，则需要传递路径参数\n    url: `/mp/v1_0/articles/${articleId}`\n  })\n}\n\n/**\n * 新建文章\n */\nexport const addArticle = (data, draft = false) => {\n  return request({\n    method: 'POST',\n    url: '/mp/v1_0/articles',\n    params: {\n      draft // 是否存为草稿（true 为草稿）\n    },\n    data\n  })\n}\n\n/**\n * 编辑文章\n */\nexport const updateArticle = (articleId, data, draft = false) => {\n  return request({\n    method: 'PUT',\n    url: `/mp/v1_0/articles/${articleId}`,\n    params: {\n      draft // 是否存为草稿（true 为草稿）\n    },\n    data\n  })\n}\n\n/**\n * 获取指定文章\n */\nexport const getArticle = articleId => {\n  return request({\n    method: 'GET',\n    url: `/mp/v1_0/articles/${articleId}`\n  })\n}\n\n/**\n * 修改文章评论状态\n */\nexport const updateCommentStatus = (articleId, allowComment) => {\n  return request({\n    method: 'PUT',\n    url: '/mp/v1_0/comments/status',\n    params: {\n      // 对象的属性名不受代码规范限制\n      article_id: articleId\n    },\n    data: {\n      allow_comment: allowComment\n    }\n  })\n}\n"]}]}